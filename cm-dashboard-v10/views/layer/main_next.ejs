    <% var b = [] %>
    <% for(device_type=0;device_type<devicetype.length;device_type++) {%>
        <% for(device_data=0;device_data<devicedata.length;device_data++) {%>
            <% var a = { "Device":'',"Devicetype":'',"Mac":'',"language":'' } %>
            <% if(devicedata[device_data].Devicetype == devicetype[device_type].id) {%>
                <% 
                a.Device = devicetype[device_type].Device;
                a.Devicetype = devicedata[device_data].Devicetype;
                a.Mac = devicedata[device_data].Mac;
                a.language = '1';
                b.push(a);
                %>
            <% } %>
        <% } %>
    <% } %>
    <% console.log(b) %>

    <!-- <div class="inline-block-center"> -->
        <div class="trapezoidNavbar">
            <div class="item left">
                <% switch(b[0].Device){
                    case "電動床": %>
                        <img class="image" src="../model/trapezoidNavbar2/pic/electricBed.png" />
                        <img class="image-active" src="../model/trapezoidNavbar2/pic/electricBed-active.png" />
                        <% break;
                    case "睡眠帶": %>
                        <img class="image" src="../model/trapezoidNavbar2/pic/sleepingRibbon.png" />
                        <img class="image-active" src="../model/trapezoidNavbar2/pic/sleepingRibbon-active.png" />
                        <% break;
                    case "血液狀態": %>
                        <img class="image" src="../model/trapezoidNavbar2/pic/blood.png" />
					    <img class="image-active" src="../model/trapezoidNavbar2/pic/blood-active.png" />
                        <% break;
                    case "基礎資料": %>
                        <img class="image" src="../model/trapezoidNavbar2/pic/info.png" />
