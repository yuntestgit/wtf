                    case "電動床": %>
                        <img class="image" src="../model/trapezoidNavbar2/pic/electricBed.png" />
                        <img class="image-active" src="../model/trapezoidNavbar2/pic/electricBed-active.png" />
                        <% break;
                    case "睡眠帶": %>
