                        <img class="image" src="../model/trapezoidNavbar2/pic/device.png" />
					    <img class="image-active" src="../model/trapezoidNavbar2/pic/device-active.png" />
                        <% break;
                } %>
                <div class="text">
                    <a onclick="getdevicedetaildata('<%= b[0].Devicetype %>','<%= b[0].Mac %>','<%= b[0].language %>')">
                    <%= b[0].Device %>
                    </a>
                </div>
            </div>
